section#sponsors
  .container
    .row
      .col-md-12.text-center
        h2.section-heading Unsere Sponsor*innen
        h3.section-subheading.text-muted
          | Die Tickets sind – nur dank ihnen – gratis!
    .row.biggest
      .col-xs-12.text-center
        .row
          a.sponsor.col-md-6(
              href='http://fh-salzburg.ac.at/',
              title='University of Applied Science Salzburg'
            )
            img.img-responsive.img-centered(
              width='555',
              height='202',
              src='images/logos/fh.jpg', 
              srcset='images/logos/fh@2x.jpg 2x',
              alt='University of Applied Sciences Salzburg'
            )
          .clearfix.visible-sm
          a.sponsor.free.col-md-6(
              href='http://www.pixelart.at/de/referenzen/',
              title='Pixelart Interactive'
            )
            img.img-responsive.img-centered(
              width='555',
              height='202',
              src='images/logos/pixelart.jpg', 
              alt='Pixelart Interactive'
            )
      .col-xs-12.text-center
        .row
          a.sponsor.col-md-4(
              href='http://findologic.com/',
              title='Findologic! Stop searching – find!'
            )
            img.img-responsive.img-centered(
              width='320',
              height='124',
              src='images/logos/findologic.png', 
              alt='Findologic'
            )
          .clearfix.visible-sm
          a.sponsor.col-md-4(
              href='https://www.vending.social/',
              title='babos zeigt. veso - vending social'
            )
            img.img-responsive.img-centered(
              width='320',
              height='124',
              src='images/logos/babos.jpg', 
              srcset='images/logos/babos@2x.jpg 2x'
              alt='babos zeigt: veso - vending social'
            )          


